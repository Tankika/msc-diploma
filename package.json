{
  "name": "msc-diploma",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "heroku-postbuild": "ng build --env=staging && tsc --project server/tsconfig.app.json",
    "start": "pm2 start dist/app.js --no-daemon",
    "clean": "rimraf dist",
    "build": "ng build --env=prod && tsc --project server/tsconfig.app.json",
    "build-start": "npm run clean && npm run build && npm start",
    "dev": "cross-env NODE_ENV=development concurrently \"ng serve --progress false --proxy-config proxy.conf.json\" \"npm run api\" ",
    "api": "cd server && nodemon --ext ts --ignore *.spec.ts --exec ts-node app",
    "test": "concurrently \"ng test --progress false\" \"npm run test-server\" ",
    "test-ng": "ng test",
    "test-server": "cd server && nodemon --ext ts --exec jasmine-ts **/*[!.api].spec.ts",
    "test-api": "cd server && nodemon --ext ts --exec jasmine-ts **/*.api.spec.ts",
    "test:once": "concurrently \"npm run test-ng:once\" \"npm run test-server:once\" \"npm run test-api:once\" ",
    "test-ng:once": "ng test -sr",
    "test-server:once": "cd server && jasmine-ts **/*[!.api].spec.ts",
    "test-api:once": "cd server && jasmine-ts **/*.api.spec.ts",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.4.1",
    "@angular/cdk": "^2.0.0-beta.10",
    "@angular/cli": "1.4.2",
    "@angular/compiler-cli": "^4.4.1",
    "@angular/common": "^4.4.1",
    "@angular/compiler": "^4.4.1",
    "@angular/core": "^4.4.1",
    "@angular/flex-layout": "^2.0.0-beta.9",
    "@angular/forms": "^4.4.1",
    "@angular/http": "^4.4.1",
    "@angular/material": "^2.0.0-beta.10",
    "@angular/platform-browser": "^4.4.1",
    "@angular/platform-browser-dynamic": "^4.4.1",
    "@angular/router": "^4.4.1",
    "async": "^2.4.0",
    "body-parser": "^1.18.1",
    "connect-mongo": "^1.3.2",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.5.1",
    "express": "^4.15.2",
    "express-session": "^1.15.2",
    "hammerjs": "^2.0.8",
    "mongoose": "^4.9.8",
    "morgan": "^1.8.1",
    "msc-diploma-bot": "github:Tankika/msc-diploma-bot",
    "pm2": "^2.7.0",
    "require-all": "^2.2.0",
    "rxjs": "^5.1.0",
    "@types/jasmine": "^2.6.0",
    "@types/mongoose": "^4.7.12",
    "@types/node": "^8.0.28",
    "winston": "^2.3.1",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@types/async": "^2.0.40",
    "@types/express": "^4.0.35",
    "@types/express-session": "^1.15.3",
    "@types/lodash": "^4.14.64",
    "@types/mongodb": "^2.2.11",
    "codelyzer": "~2.0.0",
    "concurrently": "^3.5.0",
    "cross-env": "^5.1.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "jasmine-ts": "^0.2.1",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.12.1",
    "protractor": "~5.1.0",
    "ts-node": "^3.0.6",
    "tslint": "~4.5.0",
    "typescript": "^2.3.4"
  },
  "engines": {
    "node": "8.2.1"
  }
}
